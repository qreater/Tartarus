{
    "pre_test_config_definition": {},
    "test_create_config_definition": {
        "method": "POST",
        "url": "/api/v1/config_definition/",
        "payload": {
            "config_definition_key": "test_config",
            "json_schema": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "date": {
                        "type": "string"
                    }
                }
            },
            "indexes": [
                "date"
            ]
        },
        "expected_status": 201,
        "expected_response": {
            "message": "Configuration definition created successfully.",
            "data": {
                "config_definition_key": "test_config",
                "json_schema": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "type": "string"
                        },
                        "date": {
                            "type": "string"
                        }
                    }
                },
                "indexes": [
                    "date"
                ]
            }
        }
    },
    "test_get_config_definition": {
        "method": "GET",
        "url": "/api/v1/config_definition/test_config",
        "expected_status": 200
    },
    "test_update_config_definition": {
        "method": "PUT",
        "url": "/api/v1/config_definition/test_config",
        "payload": {
            "indexes": [
                "name"
            ]
        },
        "expected_status": 200,
        "expected_response": {
            "message": "Configuration definition updated successfully.",
            "data": {
                "indexes": [
                    "name"
                ]
            }
        }
    },
    "test_delete_config_definition": {
        "method": "DELETE",
        "url": "/api/v1/config_definition/test_config",
        "expected_status": 200,
        "expected_response": {
            "message": "Configuration definition deleted successfully.",
            "data": {
                "config_definition_key": "test_config"
            }
        }
    },
    "test_list_config_definitions": {
        "method": "GET",
        "url": "/api/v1/config_definition/",
        "query": {
            "page": 1,
            "limit": 10
        },
        "expected_status": 200
    },
    "pre_test_config": {
        "config_definition_key": "test_config",
        "json_schema": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "date": {
                    "type": "string"
                }
            }
        },
        "indexes": [
            "date"
        ]
    },
    "test_create_config": {
        "method": "POST",
        "url": "/api/v1/config_definition/test_config/config",
        "payload": {
            "config_key": "test",
            "data": {
                "name": "test",
                "date": "2020-01-01"
            }
        },
        "expected_status": 201,
        "expected_response": {
            "message": "Configuration created successfully.",
            "data": {
                "name": "test",
                "date": "2020-01-01"
            }
        }
    },
    "test_get_config": {
        "method": "GET",
        "url": "/api/v1/config_definition/test_config/config/test",
        "expected_status": 200
    },
    "test_delete_config": {
        "method": "DELETE",
        "url": "/api/v1/config_definition/test_config/config/test",
        "expected_status": 200,
        "expected_response": {
            "message": "Configuration deleted successfully.",
            "data": {
                "config_definition_key": "test_config",
                "config_key": "test"
            }
        }
    },
    "test_list_configs": {
        "method": "GET",
        "url": "/api/v1/config_definition/test_config/config",
        "query": {
            "page": 1,
            "limit": 10
        },
        "expected_status": 200
    },
    "test_update_config": {
        "method": "PUT",
        "url": "/api/v1/config_definition/test_config/config/test",
        "payload": {
            "data": {
                "name": "test",
                "date": "2020-02-01"
            }
        },
        "expected_status": 200,
        "expected_response": {
            "message": "Configuration updated successfully.",
            "data": {
                "name": "test",
                "date": "2020-02-01"
            }
        }
    }
